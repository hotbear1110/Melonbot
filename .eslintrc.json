{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13
    },    
    "overrides": [
        {
            "files": ["commands/*.js"],
            "rules": {
                "no-unused-vars": "off"
            }
        }
    ],
    "plugins": [
        "react"
    ],
    "rules": {
    }
}